{
  "name": "theproject",
  "version": "1.0.0",
  "description": "socketio python flask through to Reactjs client with bfxapi websockets api",
  "main": "index.js",
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development webpack --mode development",
    "build:prod": "cross-env NODE_ENV=production webpack --mode production",
    "start": "webpack-dev-server --mode development",
    "watch": "webpack -w --mode development",
    "serve": "npm run watch && npm run start",
    "cleanWin": "rmdir /s /q node_modules",
    "cleanUnix": "rm -rf node_modules/",
    "reinstall": "npm run cleanUnix && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": "https://github.com/ThomasGuy/Bfx_Flask_SocketIO.git",
